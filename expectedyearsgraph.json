{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 300,
  "height": 300,
  "description": "Stock prices of 5 Tech Companies over Time.",
  "data": {
    "url": "https://raw.githubusercontent.com/heycurl/FIT3179/master/data/totalexpectedyears.csv"
  },
  "mark": {"type": "line", "point": true},
  "encoding": {
    "x": {"timeUnit": "year", "field": "Year", "axis": {"tickCount": 10}},
    "y": {
      "field": "Expected Number",
      "type": "quantitative",
      "title": "Expected Years of Education ",
      "axis": {"tickCount": 10}
    },
    "color": {"field": "Gender", "type": "nominal"}
  },
  "config": {}
}